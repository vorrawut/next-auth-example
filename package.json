{
  "name": "next-auth-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "keycloak:up": "docker-compose up -d",
    "keycloak:down": "docker-compose down",
    "keycloak:logs": "docker-compose logs -f keycloak",
    "keycloak:restart": "docker-compose restart keycloak",
    "keycloak:reset": "docker-compose down -v && docker-compose up -d",
    "verify": "bash scripts/verify-setup.sh",
    "test:keycloak": "node scripts/test-connection.js"
  },
  "dependencies": {
    "next": "16.0.1",
    "next-auth": "^5.0.0-beta.30",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.5",
    "typescript": "^5"
  }
